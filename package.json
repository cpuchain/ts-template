{
    "name": "ts-template",
    "version": "1.0.0",
    "description": "TypeScript template for CPUchain projects",
    "license": "MIT",
    "author": "CPUchain",
    "type": "module",
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.cjs",
            "default": "./lib/index.js"
        }
    },
    "keywords": [
        "cpuchain"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cpuchain/ts-template.git"
    },
    "scripts": {
        "lint": "eslint src/**/*.ts test/**/*.ts",
        "build": "tsc -p tsconfig.types.json --noEmit && rollup -c",
        "start": "tsx src/start.ts",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "test": "vitest && istanbul-badges-readme --colors=\"red:50,yellow:60\""
    },
    "devDependencies": {
        "@cpuchain/eslint": "^1.0.10",
        "@cpuchain/rollup": "^1.0.5",
        "@types/node": "^24.3.1",
        "@vitest/coverage-v8": "^3.2.4",
        "cross-env": "^10.0.0",
        "dotenv": "^17.2.2",
        "glob": "^11.0.3",
        "istanbul-badges-readme": "^1.9.0",
        "ts-node": "^10.9.2",
        "tsc": "^2.0.4",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "vitepress": "^1.6.4",
        "vitest": "^3.2.4"
    },
    "resolutions": {
        "fast-glob": ">=3.3.3"
    }
}
